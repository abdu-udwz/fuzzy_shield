  "version": "0.2.0",
  "configurations": [
    {
      "command": "pnpm dev",
      "name": "Run vite dev",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}/fuzzy_shield_client"
    },
    {
      "name": "Python: FastAPI Dev",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/fuzzy_shield",
      "args": [
        "fuzzy_shield.main:app",
        "--reload"
      ],
      "jinja": true
    }
  ]
}